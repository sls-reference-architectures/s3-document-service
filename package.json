    "@swc/core": "^1.12.1",
    "@swc/jest": "^0.2.38",
    "@types/jest": "^30.0.0",
    "async-retry": "^1.3.3",
    "axios": "^1.10.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
